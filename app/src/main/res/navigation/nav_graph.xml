<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/bodyMeasFragment">

    <fragment
        android:id="@+id/RegisterFragment"
        android:name="id.hendra.e_mydiabetes.ui.RegisterFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_RegisterFragment_to_landingFragment"
            app:destination="@id/landingFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_RegisterFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="id.hendra.e_mydiabetes.ui.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_landingFragment"
            app:destination="@id/landingFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_loginFragment_to_bodyMeasFragment"
            app:destination="@id/bodyMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/landingFragment"
        android:name="id.hendra.e_mydiabetes.ui.LandingFragment"
        android:label="fragment_landing"
        tools:layout="@layout/fragment_landing" >
        <action
            android:id="@+id/action_landingFragment_to_RegisterFragment"
            app:destination="@id/RegisterFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_landingFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_landingFragment_to_bodyMeasFragment"
            app:destination="@id/bodyMeasFragment"
            app:popUpToInclusive="true"/>

    </fragment>
    <fragment
        android:id="@+id/bodyMeasFragment"
        android:name="id.hendra.e_mydiabetes.ui.BodyMeasFragment"
        android:label="fragment_body_meas"
        tools:layout="@layout/fragment_body_meas" >
        <action
            android:id="@+id/action_bodyMeasFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_bodyMeasFragment_to_medFragment"
            app:destination="@id/medFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="id.hendra.e_mydiabetes.ui.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_landingFragment"
            app:destination="@id/landingFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashFragment_to_bodyMeasFragment"
            app:destination="@id/bodyMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/vitalMeasFragment"
        android:name="id.hendra.e_mydiabetes.ui.VitalMeasFragment"
        android:label="fragment_vital_meas"
        tools:layout="@layout/fragment_vital_meas" >
        <action
            android:id="@+id/action_vitalMeasFragment_to_bodyMeasFragment"
            app:destination="@id/bodyMeasFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_bloodSugarFragment"
            app:destination="@id/bloodSugarFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_bloodPressureFragment"
            app:destination="@id/bloodPressureFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_foodFragment"
            app:destination="@id/foodFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_activityFragment"
            app:destination="@id/activityFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_hbA1cFragment"
            app:destination="@id/hbA1cFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_vitalMeasFragment_to_medFragment"
            app:destination="@id/medFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/bloodPressureFragment"
        android:name="id.hendra.e_mydiabetes.ui.vital.BloodPressureFragment"
        android:label="fragment_blood_pressure"
        tools:layout="@layout/fragment_blood_pressure" >
        <action
            android:id="@+id/action_bloodPressureFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/bloodSugarFragment"
        android:name="id.hendra.e_mydiabetes.ui.vital.BloodSugarFragment"
        android:label="fragment_blood_sugar"
        tools:layout="@layout/fragment_blood_sugar" >
        <action
            android:id="@+id/action_bloodSugarFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/foodFragment"
        android:name="id.hendra.e_mydiabetes.ui.vital.FoodFragment"
        android:label="fragment_food"
        tools:layout="@layout/fragment_food" >
        <action
            android:id="@+id/action_foodFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/activityFragment"
        android:name="id.hendra.e_mydiabetes.ui.vital.ActivityFragment"
        android:label="fragment_activity"
        tools:layout="@layout/fragment_activity" >
        <action
            android:id="@+id/action_activityFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/hbA1cFragment"
        android:name="id.hendra.e_mydiabetes.ui.vital.HbA1cFragment"
        android:label="HbA1cFragment" >
        <action
            android:id="@+id/action_hbA1cFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/medFragment"
        android:name="id.hendra.e_mydiabetes.ui.MedFragment"
        android:label="fragment_med"
        tools:layout="@layout/fragment_med" >
        <action
            android:id="@+id/action_medFragment_to_vitalMeasFragment"
            app:destination="@id/vitalMeasFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_medFragment_to_bodyMeasFragment"
            app:destination="@id/bodyMeasFragment"
            app:popUpToInclusive="true"/>
    </fragment>
</navigation>