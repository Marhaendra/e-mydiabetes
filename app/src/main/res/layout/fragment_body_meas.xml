<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.BodyMeasFragment">

    <TextView
        android:id="@+id/body_meas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="98dp"
        android:layout_marginRight="16dp"
        android:text="Pengukuran Tubuh"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    
    <ImageView
        android:layout_width="36dp"
        android:layout_height="36dp"
        app:layout_constraintStart_toEndOf="@id/body_meas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/body_meas"
        app:layout_constraintBottom_toBottomOf="@id/body_meas"
        android:src="@drawable/cirle_shadow"
        android:elevation="25dp"/>

    <ImageView
        android:layout_width="36dp"
        android:layout_height="36dp"
        app:layout_constraintStart_toEndOf="@id/body_meas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/body_meas"
        app:layout_constraintBottom_toBottomOf="@id/body_meas"
        android:src="@drawable/cirle_shadow"
        android:elevation="25dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>